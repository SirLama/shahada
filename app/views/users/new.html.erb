<h1> Sign up here!!</h1>
<div class="row">
    <div class="medium-8 columns">
        <div class="medium-offset-5">
            <%= form_for(@user) do |f| %>
                <%= render 'shared/signup', f: f %>
                <%= f.submit "Sign Up", class: "button" %>
            <% end %>
        </div>
    </div>
</div>
